{
    "name": "spykapps/filament-passwordless-login",
    "description": "A highly customizable Filament 4 plugin for passwordless magic link authentication with login page, actions, resource management, and analytics widgets.",
    "keywords": [
        "laravel",
        "filament",
        "passwordless",
        "magic-link",
        "authentication",
        "filament-plugin"
    ],
    "homepage": "https://github.com/spykapps/filament-passwordless-login",
    "license": "MIT",
    "authors": [
        {
            "name": "Sanchit Patil",
            "email": "dev@spykapps.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "filament/filament": "^4.0|^5.0",
        "spykapps/passwordless-login": "^1.0"
    },
    "require-dev": {
        "orchestra/testbench": "^9.0|^10.0",
        "phpunit/phpunit": "^10.0|^11.0"
    },
    "autoload": {
        "psr-4": {
            "SpykApp\\FilamentPasswordlessLogin\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SpykApp\\FilamentPasswordlessLogin\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "SpykApp\\FilamentPasswordlessLogin\\FilamentPasswordlessLoginServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
